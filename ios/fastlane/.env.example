# App Store Connect Credentials
# Copy this file to .env and fill in your actual values
# NEVER commit .env to the repository (it's in .gitignore)

# Apple ID used for App Store Connect
FASTLANE_APPLE_ID="your_apple_id@example.com"

# Team ID (found in Apple Developer Portal)
FASTLANE_TEAM_ID="YOUR_TEAM_ID"

# App Store Connect Team ID (if different from Team ID)
FASTLANE_ITC_TEAM_ID="YOUR_ITC_TEAM_ID"

# App Bundle Identifier
APP_IDENTIFIER="YOUR_APP_BUNDLE_IDENTIFIER"

# App Store Connect API Key (Recommended method)
# Create an API Key in App Store Connect > Users and Access > Keys
APP_STORE_CONNECT_API_KEY_ID="YOUR_KEY_ID"
APP_STORE_CONNECT_API_ISSUER_ID="YOUR_ISSUER_ID"
APP_STORE_CONNECT_API_KEY_PATH="/path/to/AuthKey_XXXXX.p8"

# Alternative: App-specific password (if not using API Key)
# Generate at appleid.apple.com
# FASTLANE_APPLE_APPLICATION_SPECIFIC_PASSWORD="xxxx-xxxx-xxxx-xxxx"
